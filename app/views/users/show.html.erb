<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th>First name</th>
        <th>Last name</th>
        <th>Image</th>
        <th>date</th>
      </tr>
    </thead>
    <tbody>
      <td><%= @user.first_name %></td>
      <td><%= @user.last_name %></td>
      <td><%= image_tag @user.image_url, height: "100", width: "100"%></td>
